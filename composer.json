    "type": "library",
    "license": "GPL-3.0",
    "require": {
        "yiisoft/yii2": "2.0"
    },
    "support": {
        "issues": "https://github.com/freesoftwarefactory/yii2-easychat/issues"
    },
    "authors": [
        {
            "name": "Christian Salazar",
            "email": "christiansalazarh@gmail.com"
        }
    ],
	"autoload": {
		"psr-4": {
