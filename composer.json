            "freesoftwarefactory\\yii2easychat\\interfaces\\" : "vendor/freesoftwarefactory/yii2-easychat/interfaces",
            "freesoftwarefactory\\yii2easychat\\models\\" : "vendor/freesoftwarefactory/yii2-easychat/models",
